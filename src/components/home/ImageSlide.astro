---
import { Image } from "astro:assets";
import Marquee1 from "../../images/marquee-images/marquee1.png";
import Marquee2 from "../../images/marquee-images/marquee2.png";
import Marquee3 from "../../images/marquee-images/marquee3.png";
import Marquee4 from "../../images/marquee-images/marquee4.png";
---

<div class="flex flex-col space-y-10">
  <div class="flex pt-10">
    <Image src={Marquee1} alt="Marquee Image 1" />
    <Image src={Marquee2} alt="Marquee Image 2" />
    <Image src={Marquee3} alt="Marquee Image 2" />
    <Image src={Marquee4} alt="Marquee Image 2" />
  </div>

  <button
    class="bg-red-600 text-white font-semibold rounded-md p-3 px-7 cursor-pointer self-center"
  >
    View Projects
  </button>
</div>
